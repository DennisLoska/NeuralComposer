{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Neural Composer is a full stack web application. We used React, Node.js, Express, Tensorflow and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "theme": "cd semantic/ && npx gulp build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DennisLoska/NeuralComposer.git"
  },
  "author": "Dennis Loska",
  "license": "all rights are reserved",
  "homepage": ".",
  "engines": {
    "node": "v10.17.0",
    "npm": "6.13.2"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.0",
    "express": "^4.16.3",
    "file-loader": "^3.0.0",
    "fomantic-ui": "^2.8.2",
    "html-webpack-plugin": "^3.2.0",
    "multer": "^1.4.2",
    "node-sass": "^4.13.0",
    "react": "^16.5.2",
    "react-axe": "^3.3.0",
    "react-dom": "^16.5.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  },
  "devDependencies": {
    "concurrently": "^4.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "nodemon": "^1.17.3"
  }
}
