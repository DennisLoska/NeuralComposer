/* Collections */
@breadcrumb : 'instagram';
@form       : 'instagram';
@grid       : 'instagram';
@menu       : 'instagram';
@message    : 'instagram';
@table      : 'instagram';

/* Modules */
@accordion  : 'instagram';
@calendar   : 'instagram';
@checkbox   : 'instagram';
@dimmer     : 'instagram';
@dropdown   : 'instagram';
@embed      : 'instagram';
@modal      : 'instagram';
@nag        : 'instagram';
@popup      : 'instagram';
@progress   : 'instagram';
@slider     : 'instagram';
@rating     : 'instagram';
@search     : 'instagram';
@shape      : 'instagram';
@sidebar    : 'instagram';
