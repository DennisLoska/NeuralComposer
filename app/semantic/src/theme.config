/* Elements */
@button     : 'instagram';
@container  : 'instagram';
@divider    : 'instagram';
@emoji      : 'instagram';
@flag       : 'instagram';
@header     : 'instagram';
@icon       : 'instagram';
@image      : 'instagram';
@input      : 'instagram';
@label      : 'instagram';
@list       : 'instagram';
@loader     : 'instagram';
@placeholder: 'instagram';
@rail       : 'instagram';
@reveal     : 'instagram';
@segment    : 'instagram';
@step       : 'instagram';
